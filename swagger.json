{
  "openapi": "3.0.0",
  "info": {
    "title": "Goflow API",
    "version": "2.0.0"
  },
  "paths": {
    "/api/health": {
      "get": {
        "operationId": "health",
        "summary": "check health of the service",
        "responses": {
          "200": {
            "description": "200 response",
            "content": {
              "application/json": {
                "examples": {
                  "OK": {
                    "value": {
                      "health": "OK"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/jobs": {
      "get": {
        "operationId": "listJobs",
        "summary": "list jobs",
        "responses": {
          "200": {
            "description": "200 response",
            "content": {
              "application/json": {
                "examples": {
                  "exampleJobs": {
                    "value": {
                      "jobs": [
                        "exampleComplexAnalytics",
                        "exampleCustomOperator"
                      ]
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/jobs/{jobName}/submit": {
      "post": {
        "operationId": "submitJob",
        "summary": "submit a job for execution",
        "parameters": [
          {
            "in": "path",
            "name": "jobName"
          }
        ],
        "responses": {
          "200": {
            "description": "200 response",
            "content": {
              "application/json": {
                "examples": {
                  "customOperator": {
                    "value": {
                      "job": "exampleCustomOperator",
                      "success": true,
                      "id": "exampleCustomOperator_2023-06-21T15:02:39.943428403Z"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
